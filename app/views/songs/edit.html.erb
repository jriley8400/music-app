<h1>Editing Song #<%= @song.id %></h1>

<form action="/songs/<%= @song.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @song.name  %>" type="text" />
  </div>
  <div>
  <label for="artist_id">Artist</label><br />
    <select name="artist_id">
      <option value="<% @artist_id.each|artist_id| %>"><% @artists.each do |artists| %></option>
    </select>
  </div>
  <div>
    <input type="submit">
  </div>
</form>


