<h1>Songs</h1>



<table>
  <thead>
    <tr>
      <th>Name</th>
      <br>
      <th>Artist</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @songs.each do |song| %>
    <tr>
          <td><%= song.name %></td>
      <td><%= song.artist.name %></td>
      <td><button><a href="/songs/<%= song.id %>">Show</a></button></td>
      <td><button><a href="/songs/<%= song.id %>/edit">Edit</a></button></td>
      <td><button><a href="/songs/<%= song.id %>/destroy">Destroy</a></button></td>
    </tr>
  <% end %>
  </tbody>
</table>

